@model List<Candidature>

<h2>Candidatures</h2>

<table>
    <thead>
        <tr>
            <th>Candidat</th>
            <th>Offre</th>
            <th>Date de candidature</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var candidature in Model)
        {
            <tr>
                <td>@candidature.Candidat.Nom</td>
                <td>@candidature.Offre.Poste</td>
                <td>@candidature.DatePostulation</td>
            </tr>
        }
    </tbody>
</table>
